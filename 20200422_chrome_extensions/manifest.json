{
    "name" : "Basic Chrome guide",
    "version" : "0.9.4",
    "desription" : "Build test exstension from google guide",
    "background" : {
        "scripts" : ["js/background.js"],
        "persistent" : false
    },
    "permissions" : ["storage", "declarativeContent", "activeTab"],
    "page_action" : {
        "default_popup" : "popup.html",
        "default_icon" : {
            "16" : "img/icon16.png",
            "32" : "img/icon32.png",
            "48" : "img/icon48.png",
            "128" : "img/icon128.png"
        }
    },
    "options_page" : "options.html",
    "icons" : {
        "16" : "img/icon16.png",
        "32" : "img/icon32.png",
        "48" : "img/icon48.png",
        "128" : "img/icon128.png"
    },
    "manifest_version" : 2
}
